
import imageio.v3 as iio

filenames = ['nyan-cat1.png', 'nyan-cat2.png', 'nyan-cat3.png']
images = [ ]

#.imread() method loads an image based on the file path

for filename in filenames:
    images.append(iio.imread(filename))

#.imwrite() method to turn the images into a GIF:

iio.imwrite('nyan-cat.gif', images, duration = 100, loop = 0)
